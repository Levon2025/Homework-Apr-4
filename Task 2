#include <stdio.h>

    int axus(int num){
        for(int i = 1; i <= 10; i++){
                printf(" %d * %d = %d\n",num ,i, num * i);

            }
    
        }

        int main(){
                for(int i = 1; i <= 10; i++){
                printf(" Bazm axusyak  %d",  i);
                axus(i);
                printf(" \n");
               } 
                return 0;

            }
